//* * * * *

package sample;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int x;
        int h;
        System.out.println("Введите число x");
        x = sc.nextInt();
        System.out.println("Введите число h");
        h = sc.nextInt();
        for (int i = 1; i <= x; i++)
            for (int j = 1; j <= h; j++) {
                System.out.print("*");
                if(j>=h){
                    System.out.println();}
                }

            }

    }
