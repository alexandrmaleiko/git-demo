// С помощью цикла вычислить факториал введеный пользовтелем с клавиатуры
// при этом (4<n<16). Факториал это 5! =1*2*3*4*5=120;



package sample;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

// при этом (4<n<16). Факториал это 5! =1*2*3*4*5=120;
        int m = 1; // это фактариал и он будет начинаться с 1
        int n;

        System.out.println("Введите факториал n");
        n = sc.nextInt();

        if (4 <= n && n <= 16) {
            for (int i = 1; i <= n; i = i +1) // данный цикл выводит цифры 1,2,3,4,5,6...
            // m = m * i; // 1*2*3*4*5 =120 умножаем единицу на все цифры цикла
            {
                System.out.println(i);
            }

        }
    }
}























