package sample;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int h;
        System.out.println("Введите число h");
        h = sc.nextInt();
        for (int i = 1; i <= h; i++) {
            for (int j = 1; j <= h; j++)
                if ((((i == 1)||(i == 2 && j > 1 && j < h))||(i==3 && j==3))||
                    (((i == 4 && j > 1 && j < h))||(i==5)))
                    System.out.print("*");
                    else {
                        System.out.print(" ");
                    }

                    {
                        System.out.println();
                    }


                    //if(i==i%3){
                    // System.out.print("-");
                }
        }

    }





